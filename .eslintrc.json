{
	"extends": "airbnb",
	"parser": "babel-eslint",
	"parserOptions": {
	  "ecmaVersion": 6,
	  "ecmaFeatures": {
    	"jsx": true
    }
  },
	"env": {
		"es6": true,
		"browser": true
	},
	"rules": {
		"no-plusplus": "off",
		"react/no-danger": "off",
		"react/forbid-prop-types": "off",
		"no-param-reassign": "off"
	}
}
